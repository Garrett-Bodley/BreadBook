<%= form_for @post do |f| %>
  <%= render partial: '/partials/errors', locals: {object: @post} %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :bake %><br>
  <%= collection_select(:post, :bake_id, @current_user.bakes, :id, :name) %><br>
  <%= f.label :content %><br>
  <%= f.text_area :content %><br>
  <%= f.submit "Create Post" %><br>
<% end %>