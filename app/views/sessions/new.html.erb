<%= form_for User.new, url: '/login', method: :post do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br>
  <%= f.submit "Log In" %>
<% end %>

<%= link_to 'Signing in with the GitHub strategy', '/auth/github', method: :post%>